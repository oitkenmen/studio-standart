{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    
   "headers": [ {
      "source": "**/*.@(svg)",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=31536000"
      } ]
    }, {
      "source": "/css/c.css",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=31536000"
      } ]
    }, {
      "source": "/fonts/jet.woff2",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=31536000"
      } ]
    }, {
      "source": "/js/j.js",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=31536000"
      } ]
    } ]
  }
}
